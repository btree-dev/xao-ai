{
  "name": "xao-monorepo",
  "private": true,
  "version": "0.1.0",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "compile:contracts": "pnpm --filter @xao/contracts hardhat compile",
    "test:contracts": "pnpm --filter @xao/contracts hardhat test",
    "dev:agent": "pnpm --filter @xao/agent dev",
    "build:agent": "pnpm --filter @xao/agent build",
    "dev:web": "pnpm --filter web dev",
    "build:web": "pnpm --filter web build",
    "build:shared": "pnpm --filter @xao/shared build",
    "build:all": "pnpm build:shared && pnpm build:web && pnpm compile:contracts",
    "ci": "pnpm -r run build && pnpm --filter @xao/contracts hardhat test"
  }
}